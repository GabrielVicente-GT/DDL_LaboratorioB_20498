digraph AFD_directo {
	rankdir=LR
	A [fillcolor=lightgreen shape=circle style=filled]
	B [shape=circle]
	C [shape=circle]
	D [fillcolor=lightblue shape=doublecircle style=filled]
	E [fillcolor=lightblue shape=doublecircle style=filled]
	A -> B [label=b arrowhead=vee]
	A -> B [label=a arrowhead=vee]
	B -> B [label=b arrowhead=vee]
	B -> C [label=a arrowhead=vee]
	C -> D [label=b arrowhead=vee]
	C -> C [label=a arrowhead=vee]
	D -> E [label=c arrowhead=vee]
	D -> B [label=b arrowhead=vee]
	D -> C [label=a arrowhead=vee]
	"" [height=0 shape=box width=0]
	"" -> A [arrowhead=vee]
}
