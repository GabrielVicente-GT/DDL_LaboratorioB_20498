digraph AFD_from_AFN {
	rankdir=LR
	A [fillcolor=lightgreen shape=circle style=filled]
	B [shape=circle]
	C [shape=circle]
	D [shape=circle]
	E [shape=circle]
	F [fillcolor=lightblue shape=doublecircle style=filled]
	G [fillcolor=lightblue shape=doublecircle style=filled]
	A -> B [label=a arrowhead=vee]
	A -> C [label=b arrowhead=vee]
	B -> D [label=a arrowhead=vee]
	B -> E [label=b arrowhead=vee]
	C -> D [label=a arrowhead=vee]
	C -> E [label=b arrowhead=vee]
	D -> D [label=a arrowhead=vee]
	D -> F [label=b arrowhead=vee]
	E -> D [label=a arrowhead=vee]
	E -> E [label=b arrowhead=vee]
	F -> D [label=a arrowhead=vee]
	F -> E [label=b arrowhead=vee]
	F -> G [label=c arrowhead=vee]
	"" [height=0 shape=box width=0]
	"" -> A [arrowhead=vee]
}
